Private Sub imgDefaultKeys_Click()
10    Call Audio.PlayWave(SND_CLICK)
20        Call CustomKeys.LoadDefaults
          Dim i As Long
          
30        For i = 1 To CustomKeys.Count
40            text1(i).Text = CustomKeys.ReadableName(CustomKeys.BindedKey(i))
50        Next i
End Sub

Private Sub imgGuardar_Click()
10    Call Audio.PlayWave(SND_CLICK)
          Dim i As Long
          
20        For i = 1 To CustomKeys.Count
30            If LenB(text1(i).Text) = 0 Then
40                Call MsgBox("Hay una o más teclas no válidas, por favor verifique.", _
                      vbCritical Or vbOKOnly Or vbApplicationModal Or vbDefaultButton1, _
                      "Argentum Online")
50                Exit Sub
60            End If
70        Next i
          
80        Unload Me
End Sub

Private Sub Text1_KeyDown(Index As Integer, KeyCode As Integer, Shift As _
    Integer)
          Dim i As Long
          
10        If LenB(CustomKeys.ReadableName(KeyCode)) = 0 Then Exit Sub
          'If key is not valid, we exit
          
20        text1(Index).Text = CustomKeys.ReadableName(KeyCode)
30        text1(Index).SelStart = Len(text1(Index).Text)
          
40        For i = 1 To CustomKeys.Count
50            If i <> Index Then
60                If CustomKeys.BindedKey(i) = KeyCode Then
70                    text1(Index).Text = "" 'If the key is already assigned, simply reject it
80                    Call Beep 'Alert the user
90                    KeyCode = 0
100                   Exit Sub
110               End If
120           End If
130       Next i
          
140       CustomKeys.BindedKey(Index) = KeyCode
End Sub

Private Sub Text1_KeyPress(Index As Integer, KeyAscii As Integer)
10        KeyAscii = 0
End Sub

Private Sub Text1_KeyUp(Index As Integer, KeyCode As Integer, Shift As Integer)
10        Call Text1_KeyDown(Index, KeyCode, Shift)
End Sub

